  "private": true,
  "description": "基于React+Webpack+Typescript项目模板",
  "scripts": {
    "serve": "parcel serve ./public/index.html  --log-level=info",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build ./public/index.html  --log-level=error --no-cache --no-source-maps --profile --version"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "frontend": {
      "source": "/public/index.html"
    },
    "backend": {
      "source": "/api/index.ts"
    },
    "default": {
      "distDir": "./build",
      "publicUrl": "/"
    }
  },
  "dependencies": {
    "axios": "^1.1.3",
    "cookies-js": "^1.2.3",
    "dayjs": "^1.11.6",
    "effector": "^22.3.0",
    "effector-react": "^22.3.4",
    "events": "^3.3.0",
    "i18next": "^22.0.2",
    "md5": "^2.3.0",
    "os-browserify": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-router-dom": "^6.4.2"
  },
  "devDependencies": {
    "@parcel/transformer-yaml": "2.7.0",
    "@parcel/validator-typescript": "^2.7.0",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.11.6",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
